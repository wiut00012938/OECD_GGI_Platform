doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title GGI Uzbekistan Data
    link(rel='stylesheet', href='https://cdn.datatables.net/1.13.7/css/jquery.dataTables.min.css')
    link(rel='stylesheet', href='/static/styles/stat.css')
    link(rel='shortcut icon', href='/static/images/main_icon.png')
  body
    .head-part
      div
        a(href='/stat_ru') RU
        a.active_language(href='/stat_all') Eng
        a(href='/stat_uz') UZB
      h1.desktop-version Green Growth Indicators | Uzbekistan
      a.home_page_link(href='/', title='Home Page')  
      h1.mobile Green Growth Indicators | Uzbekistan
    .main-part
      .navigation
        h2 Navigation
        a(href='/stat_all')
          i.fa-solid.fa-file-circle-check
          h3 All indicators
        a(href='/stat-group2')
          i.fa-solid.fa-seedling
          h3 Environmental and Resource Productivity of the Economy
        a(href='/stat-group3')
          i.fa-brands.fa-envira
          h3 The Natural Asset Base
        a.active_navigation(href='/stat-group4')
          i.fa-solid.fa-building-columns
          h3 The Environmental Dimension of Quality of Life
        a(href='/stat-group5')
          i.fa-solid.fa-users-gear
          h3 Economic Opportunities and Policy Responses
        a(href='/stat-group1')
          i.fa-solid.fa-arrow-up-right-dots
          h3 Socio-economic Context
      .container-main
        .arrow#arrow
            i.fa-solid.fa-arrow-right
        .container
            .export 
              .export-message
                h2 Export 
                i.fa-solid.fa-arrow-right
              a(href='/static/data/tables-all.xlsx')
